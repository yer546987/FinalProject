@model CasinoApp.Client.Mvc.Models.ViewModels.CUTipoComidaViewModel

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>TipoComidaDto</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="tipoComida.Nombre" class="control-label"></label>
                <input asp-for="tipoComida.Nombre" class="form-control" />
                <span asp-validation-for="tipoComida.Nombre" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="tipoComida.Precio" class="control-label"></label>
                <input asp-for="tipoComida.Precio" class="form-control" />
                <span asp-validation-for="tipoComida.Precio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="tipoComida.Descripcion" class="control-label"></label>
                <input asp-for="tipoComida.Descripcion" class="form-control" />
                <span asp-validation-for="tipoComida.Descripcion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="tipoComida.TiempoInicial" class="control-label"></label>
                <input asp-for="tipoComida.TiempoInicial" class="form-control" />
                <span asp-validation-for="tipoComida.TiempoInicial" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="tipoComida.TiempoFinal" class="control-label"></label>
                <input asp-for="tipoComida.TiempoFinal" class="form-control" />
                <span asp-validation-for="tipoComida.TiempoFinal" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="tipoComida.Limite" class="control-label"></label>
                <input asp-for="tipoComida.Limite" class="form-control" />
                <span asp-validation-for="tipoComida.Limite" class="text-danger"></span>
            </div>
           
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="tipoComida.Cronograma" /> @Html.DisplayNameFor(model => model.tipoComida.Cronograma)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
